{
  "name": "n8n-nodes-telegram-multi",
  "version": "0.1.0",
  "description": "n8n community node for multiple Telegram bots support",
  "keywords": [
    "n8n-community-node-package",
    "telegram",
    "bot",
    "multiple",
    "multi"
  ],
  "license": "MIT",
  "homepage": "https://github.com/G0ld9n/n8n-nodes-telegram-multi",
  "author": "G0ld9n",
  "author": "G0ld9n",
  "author": "G0ld9n",
  "author": "G0ld9n",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/G0ld9n/n8n-nodes-telegram-multi.git"
  },
  "engines": {
    "node": ">=16.10",
    "pnpm": ">=7.1"
  },
  "packageManager": "pnpm@8.6.2",
  "main": "index.js",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier nodes credentials --write",
    "lint": "eslint nodes credentials package.json",
    "lintfix": "eslint nodes credentials package.json --fix",
    "prepublishOnly": "pnpm build && pnpm lint -o .eslintcache && pnpm test",
    "test": "jest"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/TelegramMultiApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/TelegramMulti/TelegramMulti.node.js"
    ]
  },
  "devDependencies": {
    "@types/jest": "^29.5.3",
    "@types/node": "^18.16.16",
    "@typescript-eslint/parser": "^5.59.1",
    "eslint": "^8.40.0",
    "eslint-plugin-n8n-nodes-base": "^1.11.0",
    "gulp": "^4.0.2",
    "jest": "^29.5.0",
    "n8n-workflow": "~1.2.0",
    "prettier": "^2.8.8",
    "ts-jest": "^29.1.0",
    "typescript": "^5.1.3"
  },
  "peerDependencies": {
    "n8n-workflow": "~1.2.0"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "transform": {
      "^.+\\.tsx?$": [
        "ts-jest",
        {
          "tsconfig": "tsconfig.json"
        }
      ]
    }
  }
}